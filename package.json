{
  "name": "react-lettering",
  "version": "1.0.0",
  "description": "React lettering component, radical web typography",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "watch": "nodemon gh-pages/index",
    "bundle:watch": "nodemon --exec npm run bundle:development",
    "bundle:development": "rollup -c",
    "bundle:production": "npm run bundle:development -- --environment NODE_ENV:production",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramasilveyra/react-lettering.git"
  },
  "keywords": [
    "lettering",
    "typography",
    "letter",
    "words",
    "lines",
    "react",
    "lettering.js"
  ],
  "author": "Ramiro Silveyra d'Avila <ramasilveyra1@gmail.com> (https://github.com/ramasilveyra)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ramasilveyra/react-lettering/issues"
  },
  "homepage": "https://github.com/ramasilveyra/react-lettering#readme",
  "dependencies": {
    "react": "^15.0.2 || ^0.14.x"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.10.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "nodemon": "^1.9.2",
    "react-dom": "^15.0.2",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0"
  }
}
